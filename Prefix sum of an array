// WAP to show uses of prefix sum

# include <iostream>
using namespace std;
int main()
{
    int arr[5] = {1,2,4,5,7};
    int prefix_sum[5];
    prefix_sum[0] = arr[0];
    for (int i=1 ; i<5 ; i++)
    {
        prefix_sum[i] = prefix_sum[i-1]+arr[i];
    }
    
    // Array output
    for (int i=0 ; i<5 ; i++)
    {
        cout<<prefix_sum[i]<<" ";
    }
    
    
    return 0;
}
