// Wap to check given year is leap year or not.
// Condition
// 1. If divisible by 400 then It's a leap year.
// 2. Divisible by 4 but not by 100 then it's a leap year.
// i.e 1700 is divisible by 4 but also by 100 so not a leap year.


# include <iostream>
using namespace std;

int main()
{
    cout<<"Enter year: ";
    int year;
    cin>>year;
    
    if(year%400 == 0)
    {
        cout<<year<<" is a Leap year.";
    }
    
    else
    {
        if(year%4 == 0 && year%100!=0)
        {
            cout<<year<<" is a Leap year.";
        }
        else
        {
            cout<<year<<" is not a Leap year.";
        }
    }
    
    return 0;
}
