// WAP to Find sqrt(n).

# include <iostream>
using namespace std;

void sqrt(int n)
{
    for(int i=0 ; i<n ; i++)
    {
        if(i*i==n)
        {
            cout<<"sqrt = "<<i;
            break;
        }
        else if(i*i>n)
        {
            cout<<"sqrt = "<<i-1;
            break;
        }
    }
}

int main()
{
    int n;
    cout<<"Enter a number to fins it's sqrt: ";
    cin>>n;
    sqrt(n);
    return 0;
}


